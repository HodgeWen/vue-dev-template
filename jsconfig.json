{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "apis/*": ["src/apis/*"],
      "utils/*": ["src/utils/*"],
      "views/*": ["src/views/*"],
      "style/*": ["src/style/*"],
      "components/*": ["src/components/*"],
      "images/*": ["src/assets/images/*"],
      "fonts/*": ["src/assets/fonts/*"],
      "i18n/*": ["src/i18n/*"],
      "mixins/*": ["src/mixins/*"],
      "layout/*": ["src/routes/layout/*"],
      "error/*": ["src/routes/error/*"],
      "login/*": ["src/routes/login/*"],
      "routes/*": ["src/routes/*"]
    },
    "moduleResolution": "node",
    "target": "esnext"
  },
  "include": ["src/**/*"]
}
